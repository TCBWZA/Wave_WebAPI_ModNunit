{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=NeoWarehouse;Integrated Security=true;TrustServerCertificate=True;MultipleActiveResultSets=true"
  },
  "SeedSettings": {
    "EnableSeeding": true,
    "CustomerCount": 100,
    "ProductCount": 50,
    "OrderCount": 10,
    "MinPhoneNumbersPerCustomer": 1,
    "MaxPhoneNumbersPerCustomer": 3,
    "MinOrderItemsPerOrder": 1,
    "MaxOrderItemsPerOrder": 5
  }
}
